    "        time.sleep(3)\n",
    "    count +=1\n",
    "    newdata = pd.concat([newdata, row])\n",
    "driver.close()\n",
    "newdata.set_index('id', inplace=True)\n",
    "datatoexcel = pd.ExcelWriter('facebook.xlsx')\n",
    "newdata.to_excel(datatoexcel)\n",
    "datatoexcel.save()\n",
    "newdata\n"
